environment
  tabular_graphics_data
    tabular_graphics_file = 'dakota.dat'

method
  multidim_parameter_study
    partitions = 2 2

variables
  continuous_design = 2
    descriptors     'U'       'H'
    lower_bounds    0.1       0.1
    upper_bounds    0.5       0.5

interface
	fork,
	  asynchronous 

	  # this must agree with the qsub and driver script files
	  evaluation_concurrency = 128

	analysis_driver = 'rafem_driver.sh'
  parameters_file = 'params.in'
  results_file    = 'results.out'
  file_tag

responses
  response_functions = 1
  response_descriptors = 'sea_level'
  no_gradients
  no_hessians
